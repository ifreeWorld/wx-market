/**manage.wxss**/
.weui-cells {
  width: 100%;
}

.lowTotal {
  color: #f5222d;
}

.highTotal {
  color: #52c41a;
}

.detail {
  color: #d3d3d3;
  font-size: 14px;
  display: inline;
}

.weui-cell {
  width: 100%;
  box-sizing: border-box;
}

.add {
  position: fixed;
  bottom: 100rpx;
  right: 20rpx;
  z-index: 100;
  background: #fff;
  border: 1px solid #d9d9d9;
  border-radius: 50%;
  width: 80rpx;
  height: 80rpx;
  box-shadow: 0px 10rpx 5rpx #888888;
  display: flex;
  justify-content: center;
  align-items: center;  
}

.img {
  width: 60rpx;
  height: 60rpx;
}
